<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Adminica/css/all.css")" media="screen">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Adminica/css/layout_fixed.css")" media="screen">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Adminica/css/theme/bg_squares.css")" media="screen">

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Site.css")" media="screen">

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.13.custom.min.js")" type="text/javascript"></script>
    <!-- Load Interface Plugins -->
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/uniform/jquery.uniform.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/iPhone/jquery.iphoneui.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/uitotop/js/jquery.ui.totop.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/quicksand/jquery.quicksand.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/quicksand/custom_sorter.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/quicksand/dash_sorter.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/quicksand/jquery-css-transform.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/quicksand/jquery-animate-css-rotate-scale.js")"></script>
    <!-- This file configures the various jQuery plugins for Adminica. Contains links to help pages for each plugin. -->
    <script type="text/javascript" src="@Url.Content("~/Content/Adminica/js/adminica/adminica_ui.js")"></script>
</head>
<body>
		<div id="wrapper">
            <div id="loading_overlay">
	            <div class="loading_message round_bottom">Loading...</div>
            </div>
            @*SideBar*@
                @Html.Partial("_SideBarPartial")
            @*MainContent*@
            <div>
    		    <div id="main_container" class="main_container container_16 clearfix">
            
			        @*@Html.Partial("_NavTopPartial")*@
                @RenderBody()
			    </div>
            </div>
    </div>
</body>
</html>
