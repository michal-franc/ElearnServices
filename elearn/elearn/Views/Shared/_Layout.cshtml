<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/pepper-grinder/jquery-ui-1.8.13.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CustomUi.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/MvcViews.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/SexyDropDown.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout-1.2.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.13.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SexyDropDown.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="menu">
                <ul class="topnav">
                    <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                    <li><a href="@Url.Action("List", "Course", new { pageNumber=1})">Courses</a>
                        <ul class="subnav ui-corner-bottom">
                            <li><a href="@Url.Action("MyCourses", "Course")">My Courses</a></li>
                        </ul>
                    </li>
                    <li><a href="@Url.Action("List", "Test")">Tests</a>
                        <ul class="subnav ui-corner-bottom">
                            <li><a href="@Url.Action("Finished", "Test")">Finished</a></li>
                        </ul>
                    </li>
                    <li><a href="#">Account</a>
                        <ul class="subnav ui-corner-bottom">
                            <li><a href="@Url.Action("LogOn", "Account")">Log In</a></li>
                            <li><a href="@Url.Action("LogOff", "Account")">Log Off</a></li>
                            <li><a href="@Url.Action("Register", "Account")">Register</a></li>
                        </ul>
                    </li>
                    <li><a href="@Url.Action("MyJournal", "Journal")">MyJournals</a>
                    <li><a href="#">Contact Us</a></li>
                    <li><a href="#">About</a></li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div class="footerbg">
        </div>
    </div>
</body>
</html>
